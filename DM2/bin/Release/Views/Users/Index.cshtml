@model IEnumerable<DM2.Models.Users>

@{
    ViewBag.Title = "Index";
}

<h2>Usuarios</h2>

<p>
    @Html.ActionLink("Crear usuario", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImagenURL)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ImagenURL)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IdUser }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.IdUser }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.IdUser })
        </td>
    </tr>
}

</table>
